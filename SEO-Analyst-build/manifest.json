{
   "manifest_version": 3,
   "name": "Hello Extensions",
   "description": "Base Level Extension",
   "version": "1.0",
   "action": {
      "default_popup": "popup/popup.html"
   },
   "content_scripts": [
      {
         "js": [
            "scripts/content_scripts.js"
         ],
         "matches": [
            "https://*.ir/*",
            "https://*.com/*",
            "http://*.ir/*",
            "http://*.com/*"
         ]
      }
   ]
}